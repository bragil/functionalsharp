<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
	<LangVersion>11</LangVersion>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Title>$(AssemblyName)</Title>
	<PackageTags>functional;monad;result;</PackageTags>
	<RepositoryUrl>https://github.com/bragil/functionalsharp</RepositoryUrl>
	<PackageProjectUrl>https://github.com/bragil/functionalsharp</PackageProjectUrl>
	<Description>Provides a functional programming approach to handle exceptions in an elegant way and to avoid NullReferenceExceptions.</Description>
	<AssemblyVersion>0.0.1.0</AssemblyVersion>
	<FileVersion>0.0.1.0</FileVersion>
	<Authors>bragil@gmail.com</Authors>
  </PropertyGroup>

</Project>
